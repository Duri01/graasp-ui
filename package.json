{
  "name": "@graasp/ui",
  "version": "0.2.0",
  "description": "React components for the Graasp ecosystem.",
  "author": "graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Abdallah Al Chami"
  ],
  "workspaces": [
    "example"
  ],
  "license": "MIT",
  "repository": "graasp/graasp-ui",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "start": "rimraf dist && tsc && rollup --watch -c rollup.config.js",
    "start:example": "yarn workspace graasp-ui-example start",
    "prepack": "run-s build",
    "prepare": "yarn prepack",
    "lint": "eslint .",
    "prettier:check": "prettier --check \"{src,example}/**/*.{js,ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,example}/**/*.{js,ts,tsx}\"",
    "pretest": "cd example && yarn",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 jest src --env=jsdom",
    "test:watch": "jest src --env=jsdom",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "release": "standard-version"
  },
  "dependencies": {
    "clsx": "1.1.1",
    "http-status-codes": "2.1.4",
    "immutable": "4.0.0-rc.12",
    "katex": "0.13.11",
    "react-i18next": "11.11.4",
    "react-quill": "1.3.5",
    "react-text-mask": "5.4.3"
  },
  "peerDependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.58",
    "i18next": "21.3.1",
    "react": "^16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "8.2.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/enzyme": "3.10.9",
    "@types/jest": "26.0.24",
    "@types/katex": "0.11.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-text-mask": "5.4.10",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "4.3.0",
    "jest": "24.9.0",
    "node-sass": "6.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "^2.0.4",
    "pretty-quick": "3.0.2",
    "react-query": "3.19.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.55.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-scss": "3.0.0",
    "standard-version": "8.0.2",
    "typescript": "4.1.3"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "moduleNameMapper": {
      "@material-ui/core/(.*)": "<rootDir>/example/node_modules/@material-ui/core/$1",
      "@material-ui/icons/(.*)": "<rootDir>/example/node_modules/@material-ui/icons/$1",
      "@material-ui/lab/(.*)": "<rootDir>/example/node_modules/@material-ui/lab/$1",
      "@material-ui/styles": "<rootDir>/example/node_modules/@material-ui/styles",
      "immutable": "<rootDir>/node_modules/immutable",
      "^react$": "<rootDir>/example/node_modules/react",
      "^react-dom$": "<rootDir>/example/node_modules/react-dom",
      "^react-dom/(.*)": "<rootDir>/example/node_modules/react-dom/$1",
      "\\.(css|less)$": "<rootDir>/test/styleMock.js"
    }
  },
  "packageManager": "yarn@3.0.1-rc.1"
}
